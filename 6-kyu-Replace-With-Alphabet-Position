function alphabetPosition(text) {
  let alphabet = 'abcdefghijklmnopqrstuvwxyz';
  let result =''
  
  for(let i = 0; i<text.length;i++){
    if(alphabet.indexOf(text[i].toLowerCase()) !== -1){
      let index = alphabet.indexOf(text[i].toLowerCase())
      result += (index +1) + ' '
    }
  }
  return result.trim()
}
